                                                        dotProps: {
                                                            stroke: "#444",
                                                            size: 1,
                                                            "stroke-width": 2
                                                        },
                                                        startAnimation: { // use an animation to start plotting the chart
                                                            active: true,
                                                            type: "avg",
                                                            // start from the average line.
                                                            speed: 1000,
                                                            // animate in 1 second.
                                                            easing: ">"
                                                        },
                                                        stepAnimation: { // defines an animation for data updates
                                                            speed: 2000,
                                                            delay: 0,
                                                            easing: '<>'
